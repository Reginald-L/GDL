(boundpoint 5 4)
(boundpoint 5 5)


;;; these two position are adjacent
(adjpos 1 2)
(adjpos 2 3)
(adjpos 3 4)
(adjpos 4 5)

;;; special points, in these positions, players can not do upleft, upright, etc.
(specialpoint 1 2)
(specialpoint 1 4)
(specialpoint 2 1)
(specialpoint 2 3)
(specialpoint 2 5)
(specialpoint 3 2)
(specialpoint 3 4)
(specialpoint 4 1)
(specialpoint 4 3)
(specialpoint 4 5)
