(specialpoint 5 4)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Move and moveable
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
  (<= (up ?cx ?cy ?dx ?dy ?w)
	  (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dy ?cy)
	  (not (distinct ?cx ?dx))
	  (not (boundpoint ?cx ?cy)))
  (<= (up ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dy ?cy)
	  (not (distinct ?cx ?dx))
	  (boundpoint ?cx ?cy)
	  (distinct ?cy 1))

  (<= (down ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cy ?dy)
	  (not (distinct ?cx ?dx))
	  (not (boundpoint ?cx ?cy)))
  (<= (down ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cy ?dy)
	  (not (distinct ?cx ?dx))
	  (boundpoint ?cx ?cy)
	  (distinct ?cy 5))

  (<= (left ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (not (distinct ?cy ?dy))
	  (not (boundpoint ?cx ?cy)))
  (<= (left ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (not (distinct ?cy ?dy))
	  (boundpoint ?cx ?cy)
	  (distinct ?cx 1))

  (<= (right ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cx ?dx)
	  (not (distinct ?cy ?dy))
	  (not (boundpoint ?cx ?cy)))
  (<= (right ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cx ?dx)
	  (not (distinct ?cy ?dy))
	  (boundpoint ?cx ?cy)
	  (distinct ?cx 5))

  (<= (upleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (not (boundpoint ?cx ?cy)))
  (<= (upleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 5))
	  (not (distinct ?cy 5)))
  (<= (upleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 5))
	  (not (distinct ?cy 3)))
  (<= (upleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 3))
	  (not (distinct ?cy 5)))

  (<= (upright ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
