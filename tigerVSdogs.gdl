;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Roles
;;; Initial states;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

  (role tplayer)
  (role dplayer)
  (init (point 1 1 d))
  (init (point 1 2 d))
  (init (point 1 3 d))
  (init (point 1 4 d))
  (init (point 1 5 d))
  (init (point 2 1 d))
  (init (point 2 2 b))
  (init (point 2 3 b))
  (init (point 2 4 b))
  (init (point 2 5 d))
  (init (point 3 1 d))
  (init (point 3 2 b))
  (init (point 3 3 t))
  (init (point 3 4 b))
  (init (point 3 5 d))
  (init (point 4 1 d))
  (init (point 4 2 b))
  (init (point 4 3 b))
  (init (point 4 4 b))
  (init (point 4 5 d))
  (init (point 5 1 d))
  (init (point 5 2 d))
  (init (point 5 3 d))
  (init (point 5 4 d))
  (init (point 5 5 d))
  (init (control tplayer))
  (init (eat 0))


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;; State transitions (next rules)
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; change destination point
  (<= (next (point ?dx ?dy t))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b)))
  (<= (next (point ?dx ?dy d)) 
	  (does dplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy d)
	  (true (point ?cx ?cy d))
	  (true (point ?dx ?dy b)))
;; change current point
  (<= (next (point ?cx ?cy b))
	  (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b)))
  (<= (next (point ?cx ?cy b))
	  (does dplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy d)
	  (true (point ?cx ?cy d))
	  (true (point ?dx ?dy b)))
;; other points are not changed
  (<= (next (point ?m ?n ?w))
	  (true (point ?m ?n ?w))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (not (killable ?dx ?dy ?fx ?fy ?sx ?sy))
	  (or (distinct ?m ?cx)
		  (distinct ?n ?cy))
	  (or (distinct ?m ?dx)
		  (distinct ?n ?dy)))
  (<= (next (point ?m ?n ?w))
	  (true (point ?m ?n ?w))
      (does dplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy d)
	  (true (point ?cx ?cy d))
	  (true (point ?dx ?dy b))
	  (or (distinct ?m ?cx)
		  (distinct ?n ?cy))
	  (or (distinct ?m ?dx)
		  (distinct ?n ?dy)))
;; change dog's point
  (<= (next (point ?fx ?fy b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (not (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1))
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (point ?sx ?sy b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (not (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1))
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (point ?fx ?fy b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (point ?sx ?sy b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (point ?fx1 ?fy1 b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (point ?sx1 ?sy1 b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))

  (<= (next (point ?fx ?fy b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
  (<= (next (point ?sx ?sy b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
  (<= (next (point ?fx1 ?fy1 b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
  (<= (next (point ?sx1 ?sy1 b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
  (<= (next (point ?fx2 ?fy2 b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
  (<= (next (point ?sx2 ?sy2 b))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))


;; the value of eat will be changed
  (<= (next (eat ?n))
      (true (eat ?m))
	  (succ ?m ?n)
	  (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (not (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1))
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (eat ?k))
      (true (eat ?m))
	  (succ ?m ?n)
	  (succ ?n ?k)
	  (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)))
  (<= (next (eat ?j))
      (true (eat ?m))
	  (succ ?m ?n)
	  (succ ?n ?k)
	  (succ ?n ?j)
	  (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
;; the value of eat keep
  (<= (next (eat ?m))
      (true (eat ?m))
	  (does dplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy d)
	  (true (point ?cx ?cy d))
	  (true (point ?dx ?dy b)))
  (<= (next (eat ?m))
      (true (eat ?m))
	  (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (not (killable ?dx ?dy ?fx ?fy ?sx ?sy)))
;; other points are not changed
  (<= (next (point ?m ?n ?w))
	  (true (point ?m ?n ?w))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?dx ?dy b))
	  (true (point ?cx ?cy t))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (not (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1))
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
	  (or (distinct ?m ?cx)
		  (distinct ?n ?cy))
	  (or (distinct ?m ?dx)
		  (distinct ?n ?dy))
	  (or (distinct ?m ?fx)
		  (distinct ?n ?fy))
	  (or (distinct ?m ?sx)
		  (distinct ?n ?sy)))
  (<= (next (point ?m ?n ?w))
	  (true (point ?m ?n ?w))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?dx ?dy b))
	  (true (point ?cx ?cy t))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (not (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2))
	  (or (distinct ?m ?cx)
		  (distinct ?n ?cy))
	  (or (distinct ?m ?dx)
		  (distinct ?n ?dy))
	  (or (distinct ?m ?fx)
		  (distinct ?n ?fy))
	  (or (distinct ?m ?sx)
		  (distinct ?n ?sy))
	  (or (distinct ?m ?fx1)
		  (distinct ?n ?fy1))
	  (or (distinct ?m ?sx1)
		  (distinct ?n ?sy1)))
  (<= (next (point ?m ?n ?w))
	  (true (point ?m ?n ?w))
      (does tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?dx ?dy b))
	  (true (point ?cx ?cy t))
	  (killable ?dx ?dy ?fx ?fy ?sx ?sy)
	  (twokillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1)
	  (trikillable ?dx ?dy ?fx ?fy ?sx ?sy ?fx1 ?fy1 ?sx1 ?sy1 ?fx2 ?fy2 ?sx2 ?sy2)
	  (or (distinct ?m ?cx)
		  (distinct ?n ?cy))
	  (or (distinct ?m ?dx)
		  (distinct ?n ?dy))
	  (or (distinct ?m ?fx)
		  (distinct ?n ?fy))
	  (or (distinct ?m ?sx)
		  (distinct ?n ?sy))
	  (or (distinct ?m ?fx1)
		  (distinct ?n ?fy1))
	  (or (distinct ?m ?sx1)
		  (distinct ?n ?sy1))
	  (or (distinct ?m ?fx2)
		  (distinct ?n ?fy2))
	  (or (distinct ?m ?sx2)
		  (distinct ?n ?sy2)))

;; change control
  (<= (next (control tplayer))
	  (true (control dplayer)))
  (<= (next (control dplayer))
	  (true (control tplayer)))


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Legal moves
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

  (<= (legal tplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy t)
	  (true (point ?cx ?cy t))
	  (true (point ?dx ?dy b))
	  (true (control tplayer)))
  (<= (legal dplayer (moveto ?cx ?cy ?dx ?dy))
	  (moveable ?cx ?cy ?dx ?dy d)
	  (true (point ?cx ?cy d))
	  (true (point ?dx ?dy b))
	  (true (control dplayer)))
  (<= (legal dplayer noop)
	  (true (control tplayer)))
  (<= (legal tplayer noop)
	  (true (control dplayer)))


