	  (distinct ?w b)
	  (adjpos ?cx ?dx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (not (boundpoint ?cx ?cy)))
  (<= (upright ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cx ?dx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 1))
	  (not (distinct ?cy 3)))
  (<= (upright ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cx ?dx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 1))
	  (not (distinct ?cy 5)))
  (<= (upright ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?cx ?dx)
	  (adjpos ?dy ?cy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 3))
	  (not (distinct ?cy 5)))

  (<= (downleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?cy ?dy)
	  (not (specialpoint ?cx ?cy))
	  (not (boundpoint ?cx ?cy)))
  (<= (downleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?cy ?dy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 3))
	  (not (distinct ?cy 1)))
  (<= (downleft ?cx ?cy ?dx ?dy ?w)
      (true (point ?dx ?dy b))
	  (true (point ?cx ?cy ?w))
	  (distinct ?w b)
	  (adjpos ?dx ?cx)
	  (adjpos ?cy ?dy)
	  (not (specialpoint ?cx ?cy))
	  (boundpoint ?cx ?cy)
	  (not (distinct ?cx 5))
	  (not (distinct ?cy 3)))
  (<= (downleft ?cx ?cy ?dx ?dy ?w)
